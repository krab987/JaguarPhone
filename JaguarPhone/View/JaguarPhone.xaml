<Window 
        x:Class="JaguarPhone.View.JaguarPhone"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JaguarPhone.View"
        xmlns:viewModel="clr-namespace:JaguarPhone.ViewModel"
        xmlns:controls="clr-namespace:JaguarPhone.View.Controls"
        mc:Ignorable="d"
        Title="JaguarPhone" Height="900" Width="1600">

    <Window.DataContext>
        <viewModel:MainViewModel/>
    </Window.DataContext>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="32*"/>
            <RowDefinition Height="185*"/>
        </Grid.RowDefinitions>
        <local:AdminTariff Grid.Row="1" Visibility="Collapsed"/>
        <controls:AdminServicesSuperpowers Grid.Row="1" Visibility="Collapsed"/>
        <controls:AllUser Grid.Row="1"></controls:AllUser>
        <Menu>
            
        </Menu>
    </Grid>
</Window>
